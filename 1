pipeline {

  agent any

  stages {

    stage('Checkout Source') {
      steps {
        git url: 'https://github.com/harrybhaiya/maa.git', branch :'main'
     }
    }
    

stage('Deploy App') {
      steps {
        script {
          kubernetesDeploy(configs: "nginxser.yaml", kubeconfigId: "awskubeconfigID")
        }
      }
    }


}
}
